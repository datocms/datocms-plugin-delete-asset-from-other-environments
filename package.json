{
  "name": "datocms-plugin-delete-asset-from-other-environments",
  "description": "This plugin will look for copies of an asset across all your environments and delete them in each one. This will ensure that the asset is deleted from our systems altogether, and after 24 hours, it will also be invalidated on the CDN.",
  "homepage": "https://github.com/datocms/datocms-plugin-delete-asset-from-other-environments",
  "private": false,
  "version": "0.0.1",
  "author": "Roger Tuan (DatoCMS) <r.tuan@datocms.com>",
  "type": "module",
  "keywords": [
    "datocms-plugin"
  ],
  "datoCmsPlugin": {
    "title": "Delete Asset From Other Environments",
    "coverImage": "docs/cover-1200x800.png",
    "previewImage": "docs/screenshot.png",
    "entryPoint": "dist/index.html",
    "permissions": ["currentUserAccessToken"]
  },
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "datocms-plugin-sdk": "^2.0.15",
    "datocms-react-ui": "^2.0.15",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@datocms/cma-client-browser": "^5.1.26"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "globals": "^15.9.0",
    "typescript": "^5.5.3",
    "vite": "^5.4.1"
  }
}
